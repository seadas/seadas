<?xml version="1.0" encoding="ISO-8859-1"?>
<paramInfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="http://seadas.gsfc.nasa.gov/software/schemas/ParamInfo-1.0.xsd">
    <programMetaData>
        <hasParFile>false</hasParFile>
        <parFileOptionName>NO_PAR_FILE</parFileOptionName>
    </programMetaData>
    <options>
        <option type="ifile">
            <name>infile</name>
            <value></value>
            <default></default>
            <description>is either a L1B, L2, or GEO filename</description>
            <source>command line</source>
            <aliases>
                <alias>ifile1</alias>
            </aliases>
            <order>1</order>
        </option>
        <option type="int">
            <name>SWlon</name>
            <value>1</value>
            <default>1</default>
            <description>start pixel number</description>
            <order>2</order>
        </option>
        <option type="int">
            <name>SWlat</name>
            <value>-1</value>
            <default>-1</default>
            <description>end pixel number (-1=the last pixel)</description>
            <order>3</order>
        </option>
        <option type="int">
            <name>NElon</name>
            <value>1</value>
            <default>1</default>
            <description>start line number</description>
            <order>4</order>
        </option>
        <option type="int">
            <name>NElat</name>
            <value>-1</value>
            <default>-1</default>
            <description>end line number (-1=the last line)</description>
            <order>5</order>
        </option>
      </options>
</paramInfo>