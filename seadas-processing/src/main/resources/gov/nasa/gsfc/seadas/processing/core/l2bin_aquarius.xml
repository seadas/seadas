            <validValue>
                <value>V5.0.0.RAIN</value>
                <description>V5.0.0.RAIN</description>
            </validValue>
            <validValue>
                <value>V5.0.0.smoothed</value>
                <description>V5.0.0.smoothed</description>
            </validValue>
            <validValue>
                <value>V5.0.0.RAIN.smoothed</value>
                <description>V5.0.0.RAIN.smoothed</description>
            </validValue>
            <usedAs>option</usedAs>
            <order>4</order>
        </option>
        <option type="string">
            <name>beam</name>
            <value></value>
            <default></default>
            <description>the radiometer beam (1-3)</description>
            <validValue>
                <value>1</value>
                <description>the radiometer beam 1</description>
            </validValue>
            <validValue>
                <value>2</value>
                <description>the radiometer beam 2</description>
            </validValue>
            <validValue>
                <value>3</value>
                <description>the radiometer beam 3</description>
            </validValue>

            <usedAs>option</usedAs>
            <order>5</order>
        </option>
        <option type="string">
            <name>flaguse</name>
            <value></value>
            <default></default>
            <description>Flags to apply (uses default flags if nothing entered)</description>
            <usedAs>option</usedAs>
            <order>6</order>
        </option>
        <option type="string">
            <name>filter_width</name>
            <value></value>
            <default></default>
            <description>the angular radius in degrees of the cutoff filter</description>
            <usedAs>option</usedAs>
            <order>7</order>
        </option>
