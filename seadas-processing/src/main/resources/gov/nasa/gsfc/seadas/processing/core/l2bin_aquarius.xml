            <order>0</order>
        </option>
        <option type="ofile">
            <name>ofile</name>
            <value></value>
            <default></default>
            <description>L3B output filename</description>
            <usedAs>option</usedAs>
            <order>1</order>
        </option>
        <option type="string">
            <name>resolve</name>
            <value>1D</value>
            <default>1D</default>
            <description>the bin resolution (typically '1D')</description>
            <validValue>
                <value>9</value>
                <description>9</description>
            </validValue>
            <validValue>
                <value>18</value>
                <description>18</description>
            </validValue>
            <validValue>
                <value>36</value>
                <description>36</description>
            </validValue>
            <validValue>
                <value>46</value>
                <description>46</description>
            </validValue>
            <validValue>
                <value>72</value>
                <description>72</description>
            </validValue>
            <validValue>
                <value>1D</value>
                <description>1D</description>
            </validValue>
            <validValue>
                <value>144</value>
                <description>144</description>
            </validValue>
            <usedAs>option</usedAs>
            <order>2</order>
        </option>
        <option type="string">
            <name>l3bprod</name>
            <value></value>
