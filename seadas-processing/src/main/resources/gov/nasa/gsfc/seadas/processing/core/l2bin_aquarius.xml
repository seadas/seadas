            <name>fill</name>
            <value></value>
            <default></default>
            <description>{0,1} {0 - no fill of undefined bins, 1 - fill undefined bins}</description>
            <validValue>
                <value>0</value>
                <description>0 - no fill of undefined bins</description>
            </validValue>
            <validValue>
                <value>1</value>
                <description>1 - fill undefined bins</description>
            </validValue>
            <usedAs>option</usedAs>
            <order>8</order>
        </option>
