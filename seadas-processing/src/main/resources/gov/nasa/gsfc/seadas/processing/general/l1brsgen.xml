<?xml version="1.0" encoding="ISO-8859-1"?>
<paramInfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="http://seadas.gsfc.nasa.gov/software/schemas/ParamInfo-1.0.xsd">
    <programMetaData>
        <hasParFile>true</hasParFile>
        <parFileOptionName>par</parFileOptionName>
        <hasGeoFile>true</hasGeoFile>
        <primaryOptions>
            <primaryOption>ifile</primaryOption>
            <primaryOption>ofile</primaryOption>
            <primaryOption>geofile</primaryOption>
        </primaryOptions>
    </programMetaData>
    <options>
        <option type="help">
            <name>-help</name>
            <value>false</value>
            <type>bool</type>
            <default>false</default>
            <description>print usage information</description>
            <source>default</source>
            <alias>-h</alias>
            <alias>--help</alias>
        </option>
        <option type="help">
            <name>-version</name>
            <value>false</value>
            <type>bool</type>
            <default>false</default>
            <description>print the version
                information
            </description>
            <source>default</source>
            <alias>--version</alias>
        </option>
        <option type="help">
            <name>-dump_options</name>
            <value>false</value>
            <type>bool</type>
            <default>false</default>
            <description>print
                information about each option
            </description>
            <source>default</source>
            <alias>--dump_options</alias>
        </option>
        <option type="help">
            <name>-dump_options_paramfile</name>
            <value/>
            <type>string</type>
            <description>print
                information about each option to paramfile
            </description>
            <source>default</source>
            <alias>--dump_options_paramfile</alias>
        </option>
        <option type="help">
            <name>-dump_options_xmlfile</name>
            <value>l1brsgen.xml</value>
            <type>string</type>
            <description>print
                information about each option to XML file
            </description>
            <source>command line</source>
            <alias>--dump_options_xmlfile</alias>
        </option>
        <option type="string">
            <name>pversion</name>
            <value>Unspecified</value>
            <type>string</type>
            <default>Unspecified</default>
            <description>processing version string</description>
            <source>default</source>
        </option>
        <option type="string">
            <name>suite</name>
            <value>OC</value>
            <type>string</type>
            <default>OC</default>
            <description>product suite string for loading
                suite-specific defaults
            </description>
            <source>/Users/Shared/ocssw/run/data/modisa/msl12_defaults.par</source>
        </option>

        <option type="ifile">
            <name>ifile</name>
            <value>/Users/Shared/ocssw/test/l2gen/A2002365234500.L1B_LAC</value>
            <type>string</type>
            <description>input L1 file name</description>
            <source>command line</source>
            <alias>ifile1</alias>
        </option>
        <option type="ifile">
            <name>geofile</name>
            <value/>
            <type>string</type>
            <description>input L1 geolocation file name (MODIS only)</description>
            <source>default</source>
        </option>
        <option type="ofile">
            <name>ofile</name>
            <value>output</value>
            <type>string</type>
            <default>output</default>
            <description>output L2 file #1 name, output vicarious L1B
                for inverse mode
                ofile[#] = additional output L2 file name
            </description>
            <source>default</source>
            <alias>ofile1</alias>
        </option>

        <option type="int">
            <name>spixl</name>
            <value>1</value>
            <type>int</type>
            <default>1</default>
            <description>start pixel number</description>
            <source>/Users/Shared/ocssw/run/data/modisa/l1img_defaults.par</source>
        </option>
        <option type="int">
            <name>epixl</name>
            <value> </value>
            <type>int</type>
            <default>-1</default>
            <description>end pixel number (-1=the last pixel)</description>
            <source>/Users/Shared/ocssw/run/data/modisa/l1img_defaults.par</source>
        </option>
        <option type="int">
            <name>sline</name>
            <value></value>
            <type>int</type>
            <default>1</default>
            <description>start line number</description>
            <source>/Users/Shared/ocssw/run/data/modisa/l1img_defaults.par</source>
        </option>
        <option type="int">
            <name>eline</name>
            <value></value>
            <type>int</type>
            <default>-1</default>
            <description>end line number (-1=the last line)</description>
            <source>/Users/Shared/ocssw/run/data/modisa/l1img_defaults.par</source>
        </option>
               <option type="int">
            <name>subsamp</name>
            <value>1</value>
            <type>int</type>
            <default>1</default>
            <description>sub-sampling interval</description>
            <source></source>
        </option>
               <option type="int">
            <name>rgb</name>
            <value>[1,1,1]</value>
            <type>int</type>
            <default>[1,1,1]</default>
            <description>bands to use for red, green and blue</description>
        </option>
               <option type="boolean">
            <name>atmocor</name>
            <value>true</value>
            <type>boolean</type>
            <default>true</default>
            <description>toggle atmospheric correction</description>
        </option>
        <option type="float">
            <name>datamin</name>
            <value>0.01</value>
            <type>float</type>
            <default>0.01</default>
            <description>minimum value for data scaling
                (default: see product_table)
            </description>
            <source>default</source>
            <!--<validValue>-->
                <!--<value>(default</value>-->
                <!--<description>see product_table)</description>-->
            <!--</validValue>-->
        </option>
        <option type="float">
            <name>datamax</name>
            <value>0.9</value>
            <type>float</type>
            <default>0.9</default>
            <description>maximum value for data scaling
                (default: see product_table)
            </description>
            <source>default</source>
            <!--<validValue>-->
                <!--<value>(default</value>-->
                <!--<description>see product_table)</description>-->
            <!--</validValue>-->
        </option>
        <option type="int">
            <name>stype</name>
            <value>0</value>
            <type>int</type>
            <default>0</default>
            <description>scaling type: 0: log; 1: linear
                (default: see product_table)
            </description>
            <source>default</source>
            <validValue>
                <value>0</value>
                <description>log</description>
            </validValue>
            <validValue>
                <value>1</value>
                <description>linear</description>
            </validValue>
        </option>
        <option type="int">
            <name>outmode</name>
            <value>0</value>
            <type>int</type>
            <default>0</default>
            <description>defines output file format
                0: 8bit HDF
                1: 24bit HDF
                2: PPM
                3: flat binary
                4: PNG
            </description>
            <source>default</source>
            <validValue>
                <value>0</value>
                <description>8 bit HDF browse file</description>
            </validValue>
            <validValue>
                <value>1</value>
                <description>24 bit HDF browse file</description>
            </validValue>
            <validValue>
                <value>2</value>
                <description>PPM color or PGM grayscale image file</description>
            </validValue>
            <validValue>
                <value>3</value>
                <description>flat binary</description>
            </validValue>
            <validValue>
                <value>4</value>
                <description>PNG</description>
            </validValue>
        </option>

    </options>
</paramInfo>
